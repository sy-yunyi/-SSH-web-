<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sy.qust.three.domain.Fteacher_three" table="fteacher" schema="threeup">
        <id name="tid">
            <column name="tid" sql-type="int(5)"/>
        </id>
        <property name="isGold">
            <column name="isGold" sql-type="int(1)" not-null="true"/>
        </property>
        <property name="isPass">
            <column name="isPass" sql-type="int(1)" not-null="true"/>
        </property>
        <property name="tname">
            <column name="tname" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="tphone">
            <column name="tphone" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="tpasswd">
            <column name="tpasswd" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="tage">
            <column name="tage" sql-type="int(2)" not-null="true"/>
        </property>
        <property name="tsex">
            <column name="tsex" sql-type="varchar(1)" length="1" not-null="true"/>
        </property>
        <property name="tshool">
            <column name="tshool" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="teducation">
            <column name="teducation" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="tgread">
            <column name="tgread" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="tintroduce">
            <column name="tintroduce" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="thabit">
            <column name="thabit" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="timage">
            <column name="timage" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="tpoints">
            <column name="tpoints" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="registerDate">
            <column name="registerDate" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>