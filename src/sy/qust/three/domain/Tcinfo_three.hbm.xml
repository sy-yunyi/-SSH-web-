<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sy.qust.three.domain.Tcinfo_three" table="tcinfo" schema="threeup">
        <id name="tcid">
            <column name="tcid" sql-type="int(5)"/>
        </id>
        <property name="grade">
            <column name="grade" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="tphone">
            <column name="tphone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>