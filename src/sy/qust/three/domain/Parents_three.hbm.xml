<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sy.qust.three.domain.Parents_three" table="parents" schema="threeup">
        <id name="pid">
            <column name="pid" sql-type="int(5)"/>
        </id>
        <property name="address">
            <column name="address" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="pphone">
            <column name="pphone" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="ppasswd">
            <column name="ppasswd" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="pimage">
            <column name="pimage" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="pname">
            <column name="pname" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="cname">
            <column name="cname" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="csex">
            <column name="csex" sql-type="varchar(1)" length="1" not-null="true"/>
        </property>
        <property name="cage">
            <column name="cage" sql-type="int(2)" not-null="true"/>
        </property>
        <property name="cgrade">
            <column name="cgrade" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="pclaim">
            <column name="pclaim" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>