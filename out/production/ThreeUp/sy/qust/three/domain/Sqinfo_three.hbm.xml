<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sy.qust.three.domain.Sqinfo_three" table="sqinfo" schema="threeup">
        <id name="sqid">
            <column name="sqid" sql-type="int(5)"/>
        </id>
        <property name="spphone">
            <column name="spphone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="sname">
            <column name="sname" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="sanswer">
            <column name="sanswer" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="qanswer">
            <column name="qanswer" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="sisright">
            <column name="sisright" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
        <property name="sqtime">
            <column name="sqtime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="qsubject">
            <column name="qsubject" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="qid">
            <column name="qid" sql-type="int(5)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>