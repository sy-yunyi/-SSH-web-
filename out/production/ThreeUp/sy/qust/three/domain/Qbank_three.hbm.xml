<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sy.qust.three.domain.Qbank_three" table="qbank" schema="threeup">
        <id name="qid">
            <column name="qid" sql-type="int(5)"/>
        </id>
        <property name="qnumber">
            <column name="qnumber" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="qsubject">
            <column name="qsubject" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
        <property name="qconent">
            <column name="qconent" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="qanswer">
            <column name="qanswer" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="qrank">
            <column name="qrank" sql-type="varchar(2)" length="2" not-null="true"/>
        </property>
        <property name="qanalysis">
            <column name="qanalysis" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>